{
    "name": "niblco/nibl.co.uk",
    "description": "nibl.co.uk website",
    "type": "project",
    "license": "propertiary",
    "authors": [
        {
            "name": "niblco",
            "email": "info@nibl.co.uk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-apcu": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ezyang/htmlpurifier": "^4.13",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.2",
        "myclabs/php-enum": "^1.7",
        "odan/session": "^5.1",
        "php-di/php-di": "^6.3",
        "predis/predis": "^1.1",
        "slim/http-cache": "^1.1",
        "slim/psr7": "^1.3",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
